{
  "expo": {
    "name": "Pulsoft",
    "slug": "Pulsoft",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon1024.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "NOTIFICATIONS"
      ],
      "defaultNotificationColor": "#FFFFFF",
      "package": "com.alonsin21.Pulsoft"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/favicon.png",
          "color": "#ffffff",
          "sounds": []
        }
      ],
      "@react-native-firebase/app",
      "@react-native-firebase/messaging"
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "e575dd1c-7e66-4c91-b1ed-dd4347bb9e78"
      }
    }
  }
}
